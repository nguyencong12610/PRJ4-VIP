@model IEnumerable<ShopLinhKien.Models.LeaseSoftwave>
<div class="checkbox-group required">
    @foreach (var item in Model)
    {
        <div class="form-check">
            <input class="form-check-input" type="checkbox" value="@item.ID" name="listSoftware" id="flexCheckDefault_@item.ID" >
            <label class="form-check-label" for="flexCheckDefault_@item.ID">
                @item.Name
            </label>
        </div>
    }
</div>
